{
    "name": "silasjoisten/sonata-oauth2-login-bundle",
    "description": "Login via Google with Email Validation in a Sonata Project",
    "type": "symfony-bundle",
    "require": {
        "symfony/symfony": "^2.8",
        "hwi/oauth-bundle": "^0.5",
        "google/apiclient": "^2.1",
        "twig/twig": "^1.0 || ^2.0",
        "sonata-project/user-bundle": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "symfony/phpunit-bridge": "^2.7",
        "sensio/generator-bundle": "^2.3"
    },
    "autoload" : {
        "psr-4" : {
            "SilasJoisten\\SonataOauth2LoginBundle\\" : ""
        }
    },
    "autoload-dev": {
        "psr-4": { "SilasJoisten\\SonataOauth2LoginBundle\\Tests\\": "Tests/" }
    },
