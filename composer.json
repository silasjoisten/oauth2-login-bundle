{
  "name": "silasjoisten/sonata-oauth2-login-bundle",
  "type": "symfony-bundle",
  "description": "Login via Google with Email Validation in a Sonata Project",
  "license": "MIT",
  "authors": [
    {
      "name": "Silas Joisten",
      "email": "silasjoisten@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "google/apiclient": "^2.1",
    "hwi/oauth-bundle": "^0.6.1",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug-bundle": "^1.10",
    "sonata-project/user-bundle": "^3.1 || ^4.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "matthiasnoback/symfony-dependency-injection-test": "^3.1",
    "phpunit/phpunit": "^8.2",
    "sensio/generator-bundle": "^2.3",
    "symfony/phpunit-bridge": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "SilasJoisten\\Sonata\\Oauth2LoginBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SilasJoisten\\Sonata\\Oauth2LoginBundle\\Tests\\": "tests/"
    }
  }
}
